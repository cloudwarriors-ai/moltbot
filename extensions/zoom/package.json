{
  "name": "@openclaw/zoom",
  "version": "2026.1.27-beta.1",
  "description": "OpenClaw Zoom Team Chat channel plugin",
  "type": "module",
  "dependencies": {
    "express": "^5.2.1"
  },
  "devDependencies": {
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "install": {
      "npmSpec": "@openclaw/zoom",
      "localPath": "extensions/zoom",
      "defaultChoice": "npm"
    },
    "channel": {
      "id": "zoom",
      "label": "Zoom Team Chat",
      "selectionLabel": "Zoom Team Chat (S2S OAuth)",
      "docsPath": "/channels/zoom",
      "docsLabel": "zoom",
      "blurb": "S2S OAuth; enterprise messaging.",
      "aliases": [],
      "order": 70
    },
    "extensions": [
      "./index.ts"
    ]
  }
}
