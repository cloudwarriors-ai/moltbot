# Copy to .env and fill with your credentials

# Twilio (WhatsApp)
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
# Must be a WhatsApp-enabled Twilio number, prefixed with whatsapp:
TWILIO_WHATSAPP_FROM=whatsapp:+1XXXXXXXXXX

# Docker compose required vars
CLAWDBOT_CONFIG_DIR=/root/.clawdbot
CLAWDBOT_WORKSPACE_DIR=/root/clawd
ANTHROPIC_API_KEY=sk-ant-api03-...
OPENROUTER_API_KEY=sk-or-v1-...
OPENAI_API_KEY=sk-proj-...

# GitHub 2FA OAuth
GITHUB_2FA_CLIENT_ID=
GITHUB_2FA_CLIENT_SECRET=

# ngrok
NGROK_AUTH_TOKEN=
NGROK_DOMAIN=

# Project Pulse
PROJECT_PULSE_URL=https://projectpulse.pscx.ai
PROJECT_PULSE_EMAIL=
PROJECT_PULSE_PASSWORD=

# ZW integration required vars
ZW2_URL=https://dev2.api.zoomwarriors.com
ZW2_USERNAME=
ZW2_PASSWORD=

# Zoom S2S OAuth (Team Chat App)
ZOOM_CLIENT_ID=
ZOOM_CLIENT_SECRET=
ZOOM_ACCOUNT_ID=
ZOOM_BOT_JID=
ZOOM_WEBHOOK_SECRET_TOKEN=

# Zoom S2S OAuth (Admin Reports — for chat history ingest)
# Create at marketplace.zoom.us → Build App → Server-to-Server OAuth
# Scopes needed: report:read:admin, chat_message:read:admin
ZOOM_REPORT_ACCOUNT_ID=${ZOOM_ACCOUNT_ID}
ZOOM_REPORT_CLIENT_ID=
ZOOM_REPORT_CLIENT_SECRET=
ZOOM_REPORT_USER=

ADMIN_DOMAIN=cloudwarriors.ai
ZOOM_PREFILTER_MODEL=anthropic/claude-haiku-4.5

DEV_TOOLS_API=
DEVTOOLS_API_URL=

NEXT_PUBLIC_GITHUB_BOT_API_URL=
GITHUB_BOT_API_URL=
GITHUB_BOT_API_SECRET=
GITHUB_BOT_WEBHOOK_SECRET=
ADMIN_USERS=
